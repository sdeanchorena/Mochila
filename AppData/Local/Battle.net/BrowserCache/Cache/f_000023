/**
* https://stackoverflow.com/questions/24579785/force-iframe-youtube-video-to-center-fit-and-full-cover-the-screen-in-the-backgr/60192837#60192837
* This implementation is a modification of this stack overflow answer.
* Beyond the viewport size, we're adding in some gutters above and below the video.
* Therefore anywhere we're doing a % calculation based on the size of the viewport, we need to offset that
* by an equal percentage of the gutter amount.
*/

.VideoContainer--lightbox {
  --viewer-offset: 1rem;
  --total-desired-gutter: 5rem;
  --horizontal-padding: calc(var(--total-desired-gutter) - var(--viewer-offset));
  --vertical-padding: calc(var(--total-desired-gutter) * 9 / 16);
  --horizontal-total-gutters: calc(var(--viewer-offset) + var(--horizontal-padding) + var(--horizontal-padding));
  --vertical-total-gutters: calc(var(--vertical-padding) + var(--vertical-padding));
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
  height: 100vh;
  /* Viewer in SDK doesn't have padding-right so the offset here is to make the screen even */
  width: calc(100vw - var(--viewer-offset));
  padding: var(--vertical-padding) var(--horizontal-padding)
}

.VideoContainer--lightbox .VideoPlayer {
    width: calc(100vw - var(--horizontal-total-gutters));
    height: calc(100vh - var(--vertical-total-gutters));
  }

@media (max-aspect-ratio: 16/9) {
  .VideoContainer .VideoPlayer {
    height: calc(56.25vw - var(--horizontal-total-gutters) * 9 / 16);
  }
}

@media (min-aspect-ratio: 16/9) {
  .VideoContainer .VideoPlayer {
    /* width = 100 / (9 / 16) = 177.777777 */
    width: calc(calc(100vh - var(--vertical-total-gutters)) * 16 / 9);
  }
}

.space-y-0 > :not(template) ~ :not(template) {
  --space-y-reverse: 0;
  margin-top: calc(0px * calc(1 - var(--space-y-reverse)));
  margin-bottom: calc(0px * var(--space-y-reverse));
}

.space-x-0 > :not(template) ~ :not(template) {
  --space-x-reverse: 0;
  margin-right: calc(0px * var(--space-x-reverse));
  margin-left: calc(0px * calc(1 - var(--space-x-reverse)));
}

