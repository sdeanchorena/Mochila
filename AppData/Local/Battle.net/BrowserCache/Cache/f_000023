.mask-image-gradient-to-tl {
  -webkit-mask-image: -webkit-gradient(linear, right bottom, left top, from(var(--gradient-color-stops)));
  -webkit-mask-image: linear-gradient(to top left, var(--gradient-color-stops));
}

