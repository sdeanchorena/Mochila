  /* Base definitions */
  --font-chinese-simplified: "Microsoft YaHei New", "\5FAE\8F6F\96C5\9ED1", "Microsoft YaHei", "Hei SC";
  --font-chinese-traditional: "\5FAE\8EDF\6B63\9ED1", "Microsoft JhengHei", "Tei TC";
  --font-fallback: "Segoe UI Symbol", "Segoe MDL2 Assets", "Noto Color Emoji", "Apple Color Emoji", "Segoe UI Emoji", sans-serif;
  --font-japanese: "\30E1\30A4\30EA\30AA", "Meiryo", "\30D2\30E9\30AE\30CE\89D2\30B4   Pro", "Hiragino Kaku Gothic Pro", "\6E38\30B4\30B7\30C3\30AF", "Yu Gothic";
