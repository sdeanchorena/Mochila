:root {
  --slideshow-currentSlide-duration: 200ms;
  --slideshow-nextSlide-delay: var(--slideshow-currentSlide-duration);
  --slideshow-nextSlide-duration: 500ms;
  --slideshow-nextSlide-offset: 20%;
  --slideshow-nextSlide-timing: ease-in-out;
  --slideshow-outSlide-duration: 200ms;
  --slideshow-outSlide-offset: calc(20% * -1);
  --slideshow-outSlide-timing: ease-in;
}

@-webkit-keyframes next-slide {
  from {
    opacity: 0;
    -webkit-transform: translateX(20%);
            transform: translateX(20%);
    -webkit-transform: translateX(var(--slideshow-nextSlide-offset));
            transform: translateX(var(--slideshow-nextSlide-offset));
  }

  to {
    opacity: 1;
    -webkit-transform: translateX(0);
            transform: translateX(0);
  }
}

@keyframes next-slide {
  from {
    opacity: 0;
    -webkit-transform: translateX(20%);
            transform: translateX(20%);
    -webkit-transform: translateX(var(--slideshow-nextSlide-offset));
            transform: translateX(var(--slideshow-nextSlide-offset));
  }

  to {
    opacity: 1;
    -webkit-transform: translateX(0);
            transform: translateX(0);
  }
}

@-webkit-keyframes slide-out {
  from {
    opacity: 1;
    -webkit-transform: translateX(0);
            transform: translateX(0);
  }

  to {
    opacity: 0;
    -webkit-transform: translateX(calc(20% * -1));
            transform: translateX(calc(20% * -1));
    -webkit-transform: translateX(var(--slideshow-outSlide-offset));
            transform: translateX(var(--slideshow-outSlide-offset));
  }
}

@keyframes slide-out {
  from {
    opacity: 1;
    -webkit-transform: translateX(0);
            transform: translateX(0);
  }

  to {
    opacity: 0;
    -webkit-transform: translateX(calc(20% * -1));
            transform: translateX(calc(20% * -1));
    -webkit-transform: translateX(var(--slideshow-outSlide-offset));
            transform: translateX(var(--slideshow-outSlide-offset));
  }
}

.SlideshowCarousel .Card {
    background-color: transparent;
  }

.SlideshowCarousel .CarouselSlide {
    visibility: hidden;
    --transform-translate-x: 0;
    --transform-translate-y: 0;
    --transform-rotate: 0;
    --transform-skew-x: 0;
    --transform-skew-y: 0;
    --transform-scale-x: 1;
    --transform-scale-y: 1;
    -webkit-transform: translateX(var(--transform-translate-x)) translateY(var(--transform-translate-y)) rotate(var(--transform-rotate)) skewX(var(--transform-skew-x)) skewY(var(--transform-skew-y)) scaleX(var(--transform-scale-x)) scaleY(var(--transform-scale-y));
            transform: translateX(var(--transform-translate-x)) translateY(var(--transform-translate-y)) rotate(var(--transform-rotate)) skewX(var(--transform-skew-x)) skewY(var(--transform-skew-y)) scaleX(var(--transform-scale-x)) scaleY(var(--transform-scale-y));
    --transform-translate-x: -100%;
  }

.SlideshowCarousel .CarouselSlide.is-next {
    visibility: visible;
    --transform-translate-x: 0;
    --transform-translate-y: 0;
    --transform-rotate: 0;
    --transform-skew-x: 0;
    --transform-skew-y: 0;
    --transform-scale-x: 1;
    --transform-scale-y: 1;
    -webkit-transform: translateX(var(--transform-translate-x)) translateY(var(--transform-translate-y)) rotate(var(--transform-rotate)) skewX(var(--transform-skew-x)) skewY(var(--transform-skew-y)) scaleX(var(--transform-scale-x)) scaleY(var(--transform-scale-y));
            transform: translateX(var(--transform-translate-x)) translateY(var(--transform-translate-y)) rotate(var(--transform-rotate)) skewX(var(--transform-skew-x)) skewY(var(--transform-skew-y)) scaleX(var(--transform-scale-x)) scaleY(var(--transform-scale-y));
    --transform-translate-x: 0;
    -webkit-animation: none;
            animation: none
  }

.SlideshowCarousel .CarouselSlide.is-next .Card-image, .SlideshowCarousel .CarouselSlide.is-next .Card-content {
      opacity: 0;
    }

.SlideshowCarousel .CarouselSlide.is-next .Card-image,
        .SlideshowCarousel .CarouselSlide.is-next .Card-content {
      -webkit-animation-name: next-slide;
              animation-name: next-slide;
      -webkit-animation-delay: 200ms;
              animation-delay: 200ms;
      -webkit-animation-delay: var(--slideshow-nextSlide-delay);
              animation-delay: var(--slideshow-nextSlide-delay);
      -webkit-animation-duration: 500ms;
              animation-duration: 500ms;
      -webkit-animation-duration: var(--slideshow-nextSlide-duration);
              animation-duration: var(--slideshow-nextSlide-duration);
      -webkit-animation-fill-mode: forwards;
              animation-fill-mode: forwards;
      -webkit-animation-timing-function: ease-in-out;
              animation-timing-function: ease-in-out;
      -webkit-animation-timing-function: var(--slideshow-nextSlide-timing);
              animation-timing-function: var(--slideshow-nextSlide-timing);
    }

