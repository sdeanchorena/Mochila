  --font-latin: "Noto Sans", "Tahoma", "Arial", "Helvetica";
  /* Language definitions */
  --font-lang-chinese-simplified: var(--font-latin), var(--font-chinese-simplified), var(--font-korean), var(--font-japanese), var(--font-fallback);
  --font-lang-chinese-traditional: var(--font-latin), var(--font-chinese-traditional), var(--font-korean), var(--font-japanese), var(--font-fallback);
  --font-lang-latin: var(--font-latin), var(--font-korean), var(--font-japanese), var(--font-chinese-traditional), var(--font-fallback);
